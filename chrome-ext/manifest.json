{
	"manifest_version": 2,
	
	"name": "Bargin Box",
	"description": "helps keep your shopping organized",
	"version": "0.0.1",
	
	"permissions": [
		"*://*/*"
	],
	
	"content_security_policy": "script-src 'self' http://localhost; object-src 'self'",
	
	"browser_action": {
		"default_icon": "apple-touch-icon.png",
		"default_popup": "index.html"
	}
}