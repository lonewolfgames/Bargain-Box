%tr
  %td= item.title
  %td= item.host
  %td= item.url
  %td
    %span= link_to t('actions.show'), [@cart, item]
    %span= "|"
    %span= link_to t('actions.edit'), [:edit, @cart, item]
    %span= "|"
    %span= link_to t('actions.delete'), [@cart, item], method: :delete, confirm: t('action_check')